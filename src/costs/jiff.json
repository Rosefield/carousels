{
  "parameters": [
    {"parameter": "p", "description": "number of parties"},
    {"parameter": "b", "description": "number of bits in field"}
  ],
  "metrics": [
    "Online Messages",
    "Online Rounds"
  ],
  "operations": [
    {
      "rule": {
        "nodeType": "FunctionCall",
        "match": "^<JIFFClient>\\.share"
      },
      "cost": "p-1"
    },
    {
      "rule": {
        "nodeType": "FunctionCall",
        "match": "^<SecretShare>\\.smult(<SecretShare>.*"
      },
      "cost": "p-1"
    }
  ]
}